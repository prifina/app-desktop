{
  "name": "@prifina-apps/ui-lib",
  "version": "0.0.1-alpha.0",
  "description": "UI-Lib",
  "author": "Tero <tero@prifina.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./dist && GENERATE_SOURCEMAP=true && yarn build:cjs",
    "build:esm": "cross-env NODE_ENV=development BABEL_ENV=esm babel ./src --out-dir ./dist/esm",
    "build:cjs": "cross-env NODE_ENV=development BABEL_ENV=cjs babel ./src --out-dir ./dist/cjs",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./dist/es",
    "storybook": "start-storybook",
    "test": "./node_modules/.bin/jest --setupFiles=dotenv/config --findRelatedTests ./__tests__/**.test.js",
    "test-dom": "./node_modules/.bin/jest -i --env=jest-environment-jsdom --setupFiles=dotenv/config --findRelatedTests ./__tests__/**.test.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@blend-ui/core": "1.1.68-alpha.0",
    "@blend-ui/icons": "1.1.10-alpha.0",
    "@blend-ui/modal": "1.0.14-alpha.0",
    "@blend-ui/toast": "0.0.16-alpha.0",
    "@iconify/icons-bx": "^1.2.3",
    "@iconify/icons-fe": "^1.2.1",
    "@reach/auto-id": "^0.16.0",
    "@styled-system/theme-get": "^5.1.2",
    "@svgr/cli": "^6.5.1",
    "aws-amplify": "^5.0.7",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "@blend-ui/core": "1.1.68-alpha.0",
    "@blend-ui/icons": "1.1.10-alpha.0",
    "@blend-ui/modal": "1.0.14-alpha.0",
    "@iconify/icons-bx": "^1.2.3",
    "@iconify/icons-fe": "^1.2.1",
    "@reach/auto-id": "^0.16.0",
    "@styled-system/theme-get": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5"
  },
  "dependencies": {
    "@blend-ui/avatar": "^1.0.1-alpha.0",
    "@blend-ui/icon-field": "^1.0.1-alpha.0",
    "@blend-ui/progress": "^1.0.12-alpha.0",
    "@blend-ui/search-select": "^1.0.6-alpha.0",
    "@blend-ui/toast": "^0.0.16-alpha.0",
    "@floating-ui/react-dom": "^1.3.0",
    "@iconify-icons/emojione-v1": "^1.2.4",
    "@iconify/react": "^4.1.0",
    "@prifina-apps/utils": "^0.0.12-alpha.0",
    "polished": "^4.2.2",
    "react-router": "^6.8.2",
    "zustand": "^4.1.2"
  }
}