{
  "name": "@prifina-apps/utils",
  "version": "0.0.12-alpha.0",
  "description": "Utils",
  "author": "Tero <tero@prifina.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./dist && GENERATE_SOURCEMAP=true && yarn build:cjs",
    "winbuild": "del -rf \"./dist\" && set \"GENERATE_SOURCEMAP=false\" && yarn build:cjs",
    "build:esm": "cross-env NODE_ENV=development BABEL_ENV=esm babel ./src --out-dir ./dist/esm",
    "build:cjs": "cross-env NODE_ENV=development BABEL_ENV=cjs babel ./src --out-dir ./dist/cjs",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./dist/es",
    "storybook": "start-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-jest": "^27.1.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@blend-ui/core": "1.1.68-alpha.0",
    "@blend-ui/icons": "1.1.10-alpha.0",
    "@blend-ui/modal": "1.0.14-alpha.0",
    "@iconify/icons-bx": "^1.2.3",
    "@iconify/icons-fe": "^1.2.1",
    "@reach/auto-id": "^0.16.0",
    "@styled-system/theme-get": "^5.1.2",
    "aws-amplify": "^4.3.19",
    "graphql-tag": "^2.12.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5"
  },
  "dependencies": {
    "@aws-sdk/client-appsync": "^3.43.0",
    "@blend-ui/avatar": "^1.0.1-alpha.0",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "countries-list": "^2.6.1",
    "libphonenumber-js": "1.9.51",
    "simple-zustand-devtools": "^1.1.0",
    "zustand": "^4.1.2"
  }
}